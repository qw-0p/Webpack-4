{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "no-unused-vars": "warn",
        "no-console": "warn",
        "func-names": "off",
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "indent": ["error", 4, {
            "ignoredNodes": ["JSXElement"]
        }],
        "react/jsx-indent": ["error", 4]
    },
    "env": {
        "browser": true,
        "node": true
    }
}